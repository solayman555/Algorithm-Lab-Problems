#include <stdio.h>
int main()
{
    int t,c=1;
    scanf("%d",&t);
    while(t--)
    {
        int n,max=0;
        scanf("%d",&n);
        int a[n];
        for(int i=0; i<n; i++)
        {
            scanf("%d",&a[i]);
        }
        for(int i=0; i<n; i++)
        {
            if(a[i]> max)
                max=a[i];
        }
        printf("Case %d: %d\n",c,max);
        c++;
    }

    return 0;
}
